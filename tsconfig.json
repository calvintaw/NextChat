{
	"compilerOptions": {
		"target": "ES2017",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"next-auth.d.ts",
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"app/lib/hooks/useStorage.jsx",
		"app/lib/hooks/useDarkMode.jsx",
		"app/ui/o",
		"app/lib/hooks/useToggle.jsx",
		"app/ui/general/imageForm.jsx"
, "app/ui/NewsGridWrapper.tsx"	],
	"exclude": ["node_modules"]
}
